[user]
	name = Hirofumi Wakasugi
	email = baenej@gmail.com
[color]
	ui = auto
[alias]
	aliases = !git config --get-regexp alias
	a = add
	b = branch
	c = commit
	ci = commit
	co = checkout
	d = diff
	f = fetch
	g = grep
	lg = log --graph --pretty=tformat:'%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%an %ad)%Creset'
	st = status
	oops = commit --amend --no-edit
	push-with-lease = push --force-with-lease
	review-local = "!git lg @{push}.."
	uncommit = reset --soft HEAD~1
	untrack = rm --cache --
[diff]
	mnemonicPrefix = true
	renames = true
	wordRegex = .
	submodule = log
[fetch]
	recurseSubmodules = on-demand
[grep]
	break = true
	heading = true
	lineNumber = true
[log]
	date = iso
	follow = true
[push]
	default = simple
	followTags = true
[status]
	submoduleSummary = true
	showUntrackedFiles = all
[tag]
	sort = version:refname
[include]
	path = ~/GitHub/5t111111/dotfiles/git/config.local
[init]
	defaultBranch = main
[pull]
	rebase = false
